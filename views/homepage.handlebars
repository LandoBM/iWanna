<h1>Items for Sale:</h1>
{{#if logged_in}}
{{#each products}}
<div>
    <h2><a class="links" href="/product/{{product_id}}">{{product_name}} - ${{price}}</a></h2>
</div>
{{/each}}
{{else}}
{{#each products}}
<div>
    <h2><a class="links" href="/login">{{product_name}}</a></h2>
</div>
{{/each}}
{{/if}}
